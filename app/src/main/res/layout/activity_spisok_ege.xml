<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".spisok_ege">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="366dp"
        android:layout_height="37dp"
        android:gravity="center"
        android:text="Представленная информация актуальна для абитуриентов 2022 года"
        android:textAlignment="center"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.511"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.515" />

    <Button
        android:id="@+id/nazad6"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:drawableTop="@android:drawable/btn_dialog"

        android:insetLeft="0dp"
        android:insetTop="0dp"
        android:insetRight="0dp"
        android:insetBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingEnd="1dp"

        app:icon="@android:drawable/ic_menu_close_clear_cancel"
        app:iconSize="37dp"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.957"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        app:strokeColor="#00000000" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Продолжительность ЕГЭ:"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.032" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="377dp"
        android:layout_height="637dp"
        android:text=" ✅ ЕГЭ по русскому языку: 3 часа 30 минут (210 минут) \n \n ✅ ЕГЭ по базовой математике: 3 часа (180 минут) \n \n ✅ ЕГЭ по профильной математике: 3 часа 55 минут (235 минут) \n \n ✅ ЕГЭ по обществознанию: 3 часа (180 минут) \n \n ✅ ЕГЭ по физике: 3 часа 55 минут (235 минут) \n \n ✅ ЕГЭ по информатике и ИКТ: 3 часа 55 минут (235 минут) \n \n ✅ ЕГЭ по истории: 3 часа (180 минут) \n \n ✅ ЕГЭ по химии: 3 часа 30 минут (210 минут) \n \n ✅ ЕГЭ по биологии: 3 часа 55 минут (235 минут) \n \n ✅ ЕГЭ по литературе: 3 часа 55 минут (235 минут) \n \n ✅ ЕГЭ по географии: 3 часа (180 минут) \n \n ✅  «Письменная часть» ЕГЭ по иностранным языкам, за исключением китайского: 3 часа 10 минут (190 минут) \n \n ✅  «Говорение» в ЕГЭ по иностранным языкам, за исключением китайского: 17 минут  \n \n ✅  «Письменная часть» ЕГЭ по китайскому языку: 3 часа (180 минут) \n \n ✅  «Говорение» по китайскому языку: 14 минут \n \n "
        android:textAlignment="textStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.688" />

</androidx.constraintlayout.widget.ConstraintLayout>